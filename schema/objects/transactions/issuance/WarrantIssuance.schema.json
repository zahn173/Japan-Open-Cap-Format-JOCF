{
  "$id": "https://jocf.startupstandard.org/jocf/main/schema/objects/transactions/issuance/WarrantIssuance.schema.json",
  "title": "Object - 新株予約権発行トランザクション",
  "description": "新株予約権の発行に関するトランザクション",
  "type": "object",
  "allOf": [
    {
      "$ref": "https://jocf.startupstandard.org/jocf/main/schema/primitives/objects/Object.schema.json"
    },
    {
      "$ref": "https://jocf.startupstandard.org/jocf/main/schema/primitives/objects/transactions/Transaction.schema.json"
    },
    {
      "$ref": "https://jocf.startupstandard.org/jocf/main/schema/primitives/objects/transactions/issuance/Issuance.schema.json"
    }
  ],
  "properties": {
    "object_type": {
      "const": "TX_WARRANT_ISSUANCE"
    },
    "quantity": {
      "description": "発行される新株予約権の個数",
      "$ref": "https://jocf.startupstandard.org/jocf/main/schema/types/Numeric.schema.json"
    },
    "exercise_price": {
      "description": "新株予約権1個あたりの行使時払込額",
      "$ref": "https://jocf.startupstandard.org/jocf/main/schema/types/Monetary.schema.json"
    },
    "purchase_price": {
      "description": "新株予約権1個あたりの発行価額",
      "$ref": "https://jocf.startupstandard.org/jocf/main/schema/types/Monetary.schema.json"
    },
    "total_purchace_amount": {
      "description": "新株予約権の購入時払込額合計(個数 x 発行価額)",
      "$ref": "https://jocf.startupstandard.org/jocf/main/schema/types/Monetary.schema.json"
    },
    "exercise_triggers": {
      "title": "新株予約権行使条件",
      "description": "新株予約権の更新に関する条件の一覧",
      "type": "array",
      "items": {
        "oneOf": [
          {
            "$ref": "https://jocf.startupstandard.org/jocf/main/schema/types/conversion_triggers/ElectiveConversionOnConditionTrigger.schema.json"
          },
          {
            "$ref": "https://jocf.startupstandard.org/jocf/main/schema/types/conversion_triggers/AutomaticConversionOnConditionTrigger.schema.json"
          }
        ]
      }
    },
    "anti_dilution_protection_condition": {
      "title": "新株予約権に関する希薄化防止条項",
      "type": "string"
    },
    "series_name": {
      "description": "新株予約権発行シリーズ名",
      "type": "string"
    },
    "payment_due_date": {
      "description": "払込期日",
      "type": "string",
      "format": "date"
    },
    "participation_cap_multiple": {
      "description": "参加上限額倍率",
      "$ref": "https://jocf.startupstandard.org/jocf/main/schema/types/Ratio.schema.json"
    },
    "investor_roles_and_authorities": {
      "description": "投資家の種類と権限",
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "investor_type_name": {
              "description": "投資家の種類名",
              "type": "string"
            },
            "investor_type_condition": {
              "description": "当該投資家種類とみなされるための条件",
              "type": "string"
            },
            "information_request_rights": {
              "description": "当該投資家種類が持つ情報請求権",
              "type": "string"
            },
            "preemptive_rights": {
              "description": "当該投資家種類が持つ優先引受権",
              "type": "string"
            },
            "additional_rights": {
              "description": "その他の権利",
              "type": "string"
            }
          },
          "required": [
            "investor_type_name",
            "investor_type_condition"
          ]
        }
      ]
    },
    "most_favored_nation_clause": {
      "description": "最恵待遇条項",
      "type": "string"
    },
    "transfer_restrictions_on_securityholder": {
      "description": "証券保有者の譲渡制限に関する法的文言",
      "type": "string"
    },
    "governing_law": {
      "description": "準拠法",
      "type": "string"
    },
    "exclusive_jurisdiction": {
      "description": "専属的合意管轄裁判所",
      "type": "string"
    },
    "ranking_clause": {
      "description": "債務順位。新株予約権の債務順位を記述する法的な文言",
      "type": "string"
    },
    "mandatory_redemption_attributes": {
      "description": "発行体の強制償還義務に関する項目",
      "type": "object",
      "properties": {
        "mandatory_redemption_trigger": {
          "description": "強制償還条件。強制償還条項が発動する条件を記述する法的な文言",
          "type": "string"
        },
        "mandatory_redemption_multiple": {
          "description": "強制償還倍率。発行体は、新株予約権1個ごとに、発行価額に対して当該倍率をかけた金額を支払う。",
          "$ref": "https://jocf.startupstandard.org/jocf/main/schema/types/Ratio.schema.json"
        }
      }
    },
    "representations_and_warranties_by_issuing_company": {
      "description": "発行会社による表明および保証",
      "$ref": "https://jocf.startupstandard.org/jocf/main/schema/primitives/types/IssuerRepresentationsAndWarranties.schema.json"
    },
    "self_representations_and_warranties_by_investor": {
      "description": "投資家による発行会社に対する投資家自身に関する事項の表明および保証",
      "$ref": "https://jocf.startupstandard.org/jocf/main/schema/primitives/types/InvestorSelfRepresentationsAndWarranties.schema.json"
    },
    "obligation_of_investor_to_execute_securityholders_agreement": {
      "description": "投資家の株主間契約を含む証券保有者間合意/契約への同意/締結義務",
      "type": "string"
    },
    "obligation_of_investor_to_cooperate_on_amendment": {
      "description": "投資家の投資契約の変更及び放棄への協力義務",
      "type": "string"
    },
    "id": {},
    "comments": {},
    "date": {},
    "securityholder_id": {}
  },
  "required": [
    "id",
    "date",
    "securityholder_id",
    "quantity",
    "exercise_price",
    "purchase_price",
    "exercise_triggers"
  ]
}